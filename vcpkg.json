{
  "name": "coro-http",
  "version-string": "0.1.0",
  "dependencies": [
    {
      "name": "curl",
      "default-features": false,
      "features": ["ssl", "http2", "c-ares"]
    },
    "libevent",
    "boost-regex"
  ],
  "features": {
    "stacktrace": {
      "description": "Enable stacktraces in exceptions.",
      "dependencies": [
        "boost-stacktrace",
        {
          "name": "libbacktrace",
          "platform": "!windows | mingw"
        }
      ]
    }
  }
}
